{
  "expo": {
    "name": "ACT_frontend",
    "slug": "ACT_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000000"
      },
   
    },
    "web": {
      "favicon": "./assets/logo.png"
    }
  },
  "plugins": [
    [
      "expo-barcode-scanner",
      {
        "cameraPermission": "Allow Anti-Car Theft to access camera."
      }
    ]
  ]
}
